<Window x:Class="ToolBoxControl.Dialogs.ZoomDialog"
        x:ClassModifier="internal"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ToolBoxControl.Dialogs"
             xmlns:ctrl="clr-namespace:ToolBoxControl.Controls"
             mc:Ignorable="d" 
             Height="250" Width="250">
    <ctrl:ZoomBox ScrollViewer="{Binding Path=ScrollViewer}" DesignerCanvas="{Binding Path=DesignerCanvas}" Designer="{Binding Path=Designer}"/>
</Window>
